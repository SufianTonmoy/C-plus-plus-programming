#include<iostream>
using namespace std;
int main(){
    int T;
    cin>>T;
    while(T--)
    {
        long long N;
        cin>>N;
        int ans=0;
        while(N)
        {
            ans=ans*10+N%10;
            N/=10;

        }
        cout<<ans<<endl;
    }

    return 0;
}
